{
    "name": "kilahm/attribute-router",
    "type": "library",
    "keywords": ["hhvm", "hack", "hacklang", "route", "routing"],
    "description": "A router for Hacklang that uses user defined attributes to define routing for single page applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Leinweber",
            "email": "brotchen@gmail.com"
        }
    ],
    "require-dev": {
        "hackpack/hackunit": "0.3.*"
    },
    "require": {
        "hhvm": ">=3.5",
        "hackpack/hack-class-scanner": "1.*",
        "kilahm/hack-clio": "1.1.0"
    },
    "autoload": {
        "psr-4": {
            "kilahm\\AttributeRouter\\": "src/",
            "kilahm\\AttributeRouter\\Test\\": "Test/"
        }
    },
    "bin": [
        "bin/scanroutes"
    ]
}
